pipeline {
    agent any
    stages {
      stage('Tester maven') {
    steps {
        script {
                sh "mvn -version"   }
    }
      }
        stage('clean Maven') {
            steps{
                script{
                    dir('DevOps_Project'){
                    sh "mvn clean" } 
                    }
            }
        }
          stage('Build') {
              steps {
                 script {
                    dir('DevOps_Project')
                    {sh 'mvn package -DskipTests=true'}
                }
            }
        }

        
    }
}
