pipeline {
    agent any
    stages {
      stage('Tester maven') {
    steps {
        script {
                sh "mvn -version"   }
    }
      }
        stage('clean Maven') {
            steps{
                script{
                    dir('DevOps_Project'){
                    sh "mvn clean" } 
                    }
            }
        }
        stage('Test') {
            steps {
                script {
                    dir('DevOps_Project') {
                        sh 'mvn test'
                    }
                }
            }
        }
          stage('Build') {
              steps {
                 script {
                    dir('DevOps_Project')
                    {sh 'mvn package '}
                }
            }
        }

        
    }
}
