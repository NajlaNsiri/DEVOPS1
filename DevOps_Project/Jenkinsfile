pipeline {
    agent any
    stages {
        stage("Git Clone") {
            steps {
                script {
                    git branch: 'GestionInvoice',
                        url: 'https://github.com/NajlaNsiri/DEVOPS1.git'
                }
            }
        }

      stage('Tester maven') {
    steps {
        script {
                sh "mvn -version"    
        }
    }
}
stage('cp') {
    steps {
        script {
                sh "mvn package"
            
        }
    }
}
    }
    
}
