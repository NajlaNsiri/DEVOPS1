pipeline {
    agent any
   tools {
        maven 'M2_HOME'
    }
    stages {
        stage("Git Clone") {
            steps {
                script {
                    git branch: 'GestionInvoice',
                        url: 'https://github.com/NajlaNsiri/DEVOPS1.git'
                }
            }
        }

      stage('Tester maven') {
    steps {
        script {
                sh "mvn -version"
            
        }
    }
}
stage('Clean maven') {
    steps {
        script {
             dir("DEVOPS1/blob/GestionInvoice/DevOps_Project")
            {
                sh "mvn clean"
            } 
        }
    }
}
    }
    
}
