pipeline {
    agent any
    stages {
        stage('Checkout SCM') {
            steps {
                checkout scm
            }
        }
        stage('Git Clone') {
            steps {
                script {
                    git branch: 'GestionInvoice',
                    url: 'https://github.com/NajlaNsiri/DEVOPS1.git',
                    dir('DEVOPS1/DevOps_Project') {
                        sh 'mvn clean'
                    }
                }
            }
        }
    }
}
