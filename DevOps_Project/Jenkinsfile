pipeline {
    agent any
   
    stages {
        stage("Git Clone") {
            steps {
                script {
                    git branch: 'GestionInvoice',
                        url: 'https://github.com/NajlaNsiri/DEVOPS1.git'
                }
            }
        }

        stage('Nettoyage du projet') {
            steps {
                script {
                   dir('DEVOPS1/DevOps_Project/pom.xml')
                    {sh 'mvn clean'}
                }
            }
        }   
    }
    
}
